<?xml version="1.0" encoding="utf-8"?>
<component>
    <name>org.freedesktop.IBus.Dikt</name>
    <description>Dikt Speech-to-Text</description>
    <exec>/usr/libexec/ibus-dikt-engine --ibus</exec>
    <version>@VERSION@</version>
    <author>Dikt Team</author>
    <license>MIT</license>
    <homepage>https://github.com/rohithmahesh3/Dikt</homepage>
    <textdomain>dikt-ibus</textdomain>
    <engines>
        <engine>
            <name>dikt</name>
            <language>other</language>
            <license>MIT</license>
            <author>Dikt Team</author>
            <icon>dikt</icon>
            <layout>default</layout>
            <symbol>D</symbol>
